{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Backend Debug",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/backend/dist/index.js",
      "preLaunchTask": "backend-build",
      "outFiles": ["${workspaceFolder}/backend/dist/**/*.js"],
      "cwd": "${workspaceFolder}/backend"
